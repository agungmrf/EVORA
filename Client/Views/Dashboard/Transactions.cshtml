@{
    ViewData["Title"] = "Dashboard Admin - Transactions";
    Layout = "_Dashboard";
}
<div class="card-body px-4 py-3">
    <div class="row align-items-center">
        <div class="col-9">
            <h4 class="fw-semibold mb-8">History Transactions</h4>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a class="text-muted" asp-action="Index" asp-controller="Dashboard">Dashboard</a></li>
                    <li class="breadcrumb-item" aria-current="page">Transactions</li>
                </ol>
            </nav>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 d-flex">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-8">
                        <h5 class="card-title fw-semibold mb-4">Table Locations</h5>
                    </div>
                    <p class="mb-0">This is a sample page </p>
                </div>
            </div>
        </div>
    </div>
</div>

